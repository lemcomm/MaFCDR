{% extends "layout.html.twig" %}

{% block content %}
<h2>{{ 'assoc.route.wordsDeity.name'|trans({},"orgs")|title }}</h2>
<p>{{ 'assoc.route.wordsDeity.intro'|trans({},"orgs")|raw }}</p>

{{ form_start(form) }}
{{ form_errors(form) }}
<dl>
	<dt>{{ form_label(form.words)}}</dt><dd>{{ form_widget(form.words) }}</dd>
	{{ form_widget(form.submit) }}
{{ form_end(form) }}
</dl>

{% endblock %}
